# Environment variables for MindWell PWA

# App Configuration
REACT_APP_NAME=MindWell
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Security Configuration
REACT_APP_ENCRYPTION_ITERATIONS=100000
REACT_APP_SESSION_TIMEOUT=3600000

# Crisis Support Configuration
REACT_APP_CRISIS_DETECTION_ENABLED=true
REACT_APP_DEFAULT_COUNTRY_CODE=US

# Analytics Configuration (Optional)
REACT_APP_ANALYTICS_ENABLED=false
REACT_APP_ANALYTICS_ID=

# Deployment Configuration
REACT_APP_BASE_URL=http://localhost:3000
REACT_APP_API_BASE_URL=

# Vercel Configuration (for deployment)
VERCEL_ORG_ID=
VERCEL_PROJECT_ID=
VERCEL_TOKEN=

# Feature Flags
REACT_APP_VR_ENABLED=true
REACT_APP_VOICE_NOTES_ENABLED=true
REACT_APP_ML_MODELS_ENABLED=true
REACT_APP_BEHAVIORAL_ANALYTICS_ENABLED=true

# Internationalization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,hi

# Performance Configuration
REACT_APP_BUNDLE_SIZE_LIMIT=400
REACT_APP_MEMORY_LIMIT=50
REACT_APP_CACHE_DURATION=86400000
